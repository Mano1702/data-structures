 }
 }
 void display()
 {
     int i=1;
     for(i=1;i<=2;i++)
     {
         printf("the elements of node%d\n",i);
     tempi=headi;
     while(tempi->nexti!=0)
     {
         printf("%d\n",tempi->datai);
         tempi=tempi->nexti;
     }
     printf("%d\n",tempi->datai);
