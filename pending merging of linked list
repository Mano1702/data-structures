     {
     newnode=malloc(sizeof(struct node i));
     printf("enter the data of node%d ",i);
     scanf("%d",&newnodei->datai);
     
     newnode i->next i=0;
     if (head i==0)
     {
        headi=taili=newnodei;
     }
     else
     {
         
         taili->nexti=newnodei;
