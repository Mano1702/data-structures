#include<stdio.h>
#include<stdlib.h>
int a[20];
int main()
{
  printf("enter the array size");

  int num,i,front=-1,rear=-1;
  scanf("%d ",&num);
  int a[num];

  for(i=0;i<num;i++)
  {
      scanf("%d",&a[i]);
      front++;
  }


 for( i=0;i<=front;i++)
  {
      printf("%d ",a[i]);
  }
 front--=rear;

printf("\nAfter pop\n");

  for( i=0;i<=rear;i++)
  {
      printf("%d ",a[i]);
  }




  }
